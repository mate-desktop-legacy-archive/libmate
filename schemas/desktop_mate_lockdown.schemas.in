<?xml version="1.0"?>
<mateconfschemafile>
  <schemalist>

    <schema>
      <key>/schemas/desktop/mate/lockdown/disable_command_line</key>
      <applyto>/desktop/mate/lockdown/disable_command_line</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Disable command line</short>
        <long>
          Prevent the user from accessing the terminal or specifying
          a command line to be executed. For example, this would
          disable access to the panel's "Run Application" dialog.
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/desktop/mate/lockdown/disable_save_to_disk</key>
      <applyto>/desktop/mate/lockdown/disable_save_to_disk</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Disable saving files to disk</short>
        <long>
          Prevent the user from saving files to disk. For example,
          this would disable access to all applications' "Save as"
          dialogs.
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/desktop/mate/lockdown/disable_printing</key>
      <applyto>/desktop/mate/lockdown/disable_printing</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Disable printing</short>
        <long>
          Prevent the user from printing. For example, this would
          disable access to all applications' "Print" dialogs.
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/desktop/mate/lockdown/disable_print_setup</key>
      <applyto>/desktop/mate/lockdown/disable_print_setup</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Disable print setup</short>
        <long>
         Prevent the user from modifying print settings. For example,
         this would disable access to all applications' "Print Setup"
         dialogs.
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/desktop/mate/lockdown/disable_user_switching</key>
      <applyto>/desktop/mate/lockdown/disable_user_switching</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Disable user switching</short>
        <long>
         Prevent the user from switching to another account while his
         session is active.
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/desktop/mate/lockdown/disable_lock_screen</key>
      <applyto>/desktop/mate/lockdown/disable_lock_screen</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Disable lock screen</short>
        <long>
         Prevent the user to lock his screen.
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/desktop/mate/lockdown/disable_application_handlers</key>
      <applyto>/desktop/mate/lockdown/disable_application_handlers</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Disable URL and MIME type handlers</short>
        <long>
         Prevent running any URL or MIME type handler applications.
        </long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/desktop/mate/lockdown/disable_application_handlers</key>
      <applyto>/desktop/mate/lockdown/disable_application_handlers</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Disable URL and MIME type handlers</short>
        <long>
         Prevent running any URL or MIME type handler applications.
        </long>
      </locale>
    </schema>

  </schemalist>
</mateconfschemafile>
