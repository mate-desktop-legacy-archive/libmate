<mateconfschemafile>
    <schemalist>
      <schema>
         <key>/schemas/desktop/mate/applications/at/visual/exec</key>
         <applyto>/desktop/mate/applications/at/visual/exec</applyto>
         <owner>mate</owner>
	 <type>string</type>
         <default>orca</default>
         <locale name="C">
            <short>Preferred Visual assistive technology application</short>
            <long>Preferred Visual assistive technology application be used for login, menu, or command line.</long>
         </locale>
      </schema>    
      <schema>
         <key>/schemas/desktop/mate/applications/at/visual/startup</key>
         <applyto>/desktop/mate/applications/at/visual/startup</applyto>
         <owner>mate</owner>
	 <type>bool</type>
         <default>false</default>
         <locale name="C">
            <short>Start preferred Visual assistive technology application</short>
            <long>MATE to start preferred Visual assistive technology application during login.</long>
         </locale>
      </schema>    
    </schemalist>
</mateconfschemafile>
