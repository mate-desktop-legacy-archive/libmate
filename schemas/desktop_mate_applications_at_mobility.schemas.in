<mateconfschemafile>
    <schemalist>
      <schema>
         <key>/schemas/desktop/mate/applications/at/mobility/exec</key>
         <applyto>/desktop/mate/applications/at/mobility/exec</applyto>
         <owner>mate</owner>
	 <type>string</type>
         <default>dasher</default>
         <locale name="C">
            <short>Preferred Mobility assistive technology application</short>
            <long>Preferred Mobility assistive technology application to be used for login, menu, or command line.</long>
         </locale>
      </schema>    
      <schema>
         <key>/schemas/desktop/mate/applications/at/mobility/startup</key>
         <applyto>/desktop/mate/applications/at/mobility/startup</applyto>
         <owner>mate</owner>
	 <type>bool</type>
         <default>false</default>
         <locale name="C">
            <short>Start preferred Mobility assistive technology application</short>
            <long>MATE to start preferred Mobility assistive technology application during login.</long>
         </locale>
      </schema>    
    </schemalist>
</mateconfschemafile>
