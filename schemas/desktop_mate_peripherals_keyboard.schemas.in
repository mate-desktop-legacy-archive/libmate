<?xml version="1.0"?>
<mateconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/repeat</key>
      <applyto>/desktop/mate/peripherals/keyboard/repeat</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short></short>
        <long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/click</key>
      <applyto>/desktop/mate/peripherals/keyboard/click</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short></short>
        <long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/rate</key>
      <applyto>/desktop/mate/peripherals/keyboard/rate</applyto>
      <owner>mate</owner>
      <type>int</type>
      <default>30</default>
      <locale name="C">
        <short></short>
        <long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/delay</key>
      <applyto>/desktop/mate/peripherals/keyboard/delay</applyto>
      <owner>mate</owner>
      <type>int</type>
      <default>500</default>
      <locale name="C">
        <short></short>
        <long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/click_volume</key>
      <applyto>/desktop/mate/peripherals/keyboard/click_volume</applyto>
      <owner>mate</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short></short>
        <long></long>
      </locale>
    </schema>
    <!-- Keyboard bell -->
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/bell_mode</key>
      <applyto>/desktop/mate/peripherals/keyboard/bell_mode</applyto>
      <owner>mate</owner>
      <type>string</type>
      <default>on</default>
      <locale name="C">
        <short></short>
        <long>possible values are "on", "off", and "custom".</long>
      </locale>
    </schema>
<!--
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/bell_percent</key>
      <applyto>/desktop/mate/peripherals/keyboard/bell_percent</applyto>
      <owner>mate</owner>
      <type>int</type>
      <default>50</default>
      <locale name="C">
        <short></short>
        <long></long>
      </locale>
    </schema>
-->
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/bell_pitch</key>
      <applyto>/desktop/mate/peripherals/keyboard/bell_pitch</applyto>
      <owner>mate</owner>
      <type>int</type>
      <default>400</default>
      <locale name="C">
        <short></short>
        <long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/bell_duration</key>
      <applyto>/desktop/mate/peripherals/keyboard/bell_duration</applyto>
      <owner>mate</owner>
      <type>int</type>
      <default>100</default>
      <locale name="C">
        <short></short>
        <long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/bell_custom_file</key>
      <applyto>/desktop/mate/peripherals/keyboard/bell_custom_file</applyto>
      <owner>mate</owner>
      <type>string</type>
      <locale name="C">
        <short>Keyboard Bell Custom Filename</short>
        <long>File name of the bell sound to be played.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/peripherals/keyboard/remember_numlock_state</key>
      <applyto>/desktop/mate/peripherals/keyboard/remember_numlock_state</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Remember NumLock state</short>
        <long>When set to true, MATE will remember the state of the NumLock LED between sessions.</long>
      </locale>
    </schema>
  </schemalist>
</mateconfschemafile>
