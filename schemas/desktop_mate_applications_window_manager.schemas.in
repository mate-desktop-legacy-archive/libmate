<?xml version="1.0"?>
<mateconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/desktop/mate/applications/window_manager/default</key>
      <applyto>/desktop/mate/applications/window_manager/default</applyto>
      <owner>mate</owner>
      <type>string</type>
      <locale name="C">
        <short>Fallback window manager (deprecated)</short>
        <long>
          Fallback window manager if user window manager can't be found.
          This key has been deprecated since MATE 2.12.
        </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/applications/window_manager/current</key>
      <applyto>/desktop/mate/applications/window_manager/current</applyto>
      <owner>mate</owner>
      <type>string</type>
      <locale name="C">
        <short>User window manager (deprecated)</short>
        <long>
          Window manager to try first.
          This key has been deprecated since MATE 2.12.
        </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/applications/window_manager/number_of_workspaces</key>
      <applyto>/desktop/mate/applications/window_manager/number_of_workspaces</applyto>
      <owner>mate</owner>
      <type>int</type>
      <locale name="C">
        <short>The number of workspaces (deprecated)</short>
        <long>
          The number of workspaces the window manager should use
          This key has been deprecated since MATE 2.12.
        </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/applications/window_manager/workspace_names</key>
      <applyto>/desktop/mate/applications/window_manager/workspace_names</applyto>
      <owner>mate</owner>
      <type>list</type>
      <list_type>string</list_type>
      <locale name="C">
        <short>Names of the workspaces (deprecated)</short>
        <long>
          A list with names of the first window manager workspaces.
          This key has been deprecated since MATE 2.12.
        </long>
      </locale>
    </schema>
  </schemalist>
</mateconfschemafile>
