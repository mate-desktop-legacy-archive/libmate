<?xml version="1.0"?>
<mateconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/desktop/mate/applications/calendar/exec</key>
      <applyto>/desktop/mate/applications/calendar/exec</applyto>
      <owner>mate</owner>
      <type>string</type>
      <default>evolution</default>
      <locale name="C">
        <short>Default calendar</short>
        <long>Default calendar application</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/applications/calendar/needs_term</key>
      <applyto>/desktop/mate/applications/calendar/needs_term</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Calendar needs terminal</short>
        <long>Whether the default calendar application needs a terminal to run</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/desktop/mate/applications/tasks/exec</key>
      <applyto>/desktop/mate/applications/tasks/exec</applyto>
      <owner>mate</owner>
      <type>string</type>
      <default>evolution</default>
      <locale name="C">
        <short>Default tasks</short>
        <long>Default tasks application</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/mate/applications/tasks/needs_term</key>
      <applyto>/desktop/mate/applications/tasks/needs_term</applyto>
      <owner>mate</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Tasks needs terminal</short>
        <long>Whether the default tasks application needs a terminal to run</long>
      </locale>
    </schema>
  </schemalist>
</mateconfschemafile>
