msgid "Picture Filename"
msgstr "Názov súboru obrázka"

#: ../schemas/desktop_mate_background.schemas.in.in.h:10
msgid "Picture Opacity"
msgstr "Priehľadnosť obrázka"

#: ../schemas/desktop_mate_background.schemas.in.in.h:11
msgid "Picture Options"
msgstr "Voľby obrázka"

#: ../schemas/desktop_mate_background.schemas.in.in.h:12
msgid "Primary Color"
msgstr "Primárna farba"

#: ../schemas/desktop_mate_background.schemas.in.in.h:13
